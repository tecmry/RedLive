<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleView">

        <attr name="shape" format="enum">
            <enum name="rect" value="1"/>
            <enum name="circle" value="2"/>
        </attr>
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="cover_color" format="color" />
        <attr name="round_radius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ExpandTextView">
        <attr name="maxExpandLines" format="integer"/>
        <attr name="duration" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TextFieldBoxes">
        <attr name="enabled" format="boolean" />
        <attr name="hint" format="string" />
        <attr name="text" format="string" />
        <attr name="singleLine" format="boolean" />
        <attr name="maxLines" format="integer" />
        <attr name="maxCharacters" format="integer" />
        <attr name="minCharacters" format="integer" />
        <attr name="helperText" format="string" />
        <attr name="helperTextColor" format="color" />
        <attr name="errorColor" format="color" />
        <attr name="primaryColor" format="color" />
        <attr name="hasFocus" format="boolean" />
    </declare-styleable>
</resources>